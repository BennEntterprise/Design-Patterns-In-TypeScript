  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/node": "^22.5.0",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.24.0",
    "missing-tests": "^2.1.1",
    "ts-jest": "^29.2.5",
